<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from UserLogicService WSDL service -->

  <service name="UserLogic" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://logic"/>
      <parameter name="wsdlServiceElement" value="UserLogicService"/>
      <parameter name="schemaQualified" value="http://logic"/>
      <parameter name="wsdlServicePort" value="UserLogic"/>
      <parameter name="className" value="logic.UserLogicSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="UserLogic"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getUserInfo" qname="operNS:getUserInfo" xmlns:operNS="http://logic" returnQName="retNS:getUserInfoReturn" xmlns:retNS="http://logic" returnType="rtns:anyType" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:user" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="login" qname="operNS:login" xmlns:operNS="http://logic" returnQName="retNS:loginReturn" xmlns:retNS="http://logic" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:username" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="addUser" qname="operNS:addUser" xmlns:operNS="http://logic" returnQName="retNS:addUserReturn" xmlns:retNS="http://logic" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:username" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:name" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:birthdate" xmlns:pns="http://logic" type="tns:date" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:gender" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getUserNames" qname="operNS:getUserNames" xmlns:operNS="http://logic" returnQName="retNS:getUserNamesReturn" xmlns:retNS="http://logic" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://logic" returnItemQName="tns:item" xmlns:tns="http://logic" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://logic" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getUserNames addUser getUserInfo login"/>

      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>addUserResponse"
        type="java:logic.AddUserResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://logic"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>getUserInfo"
        type="java:logic.GetUserInfo"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>getUserNames"
        type="java:logic.GetUserNames"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>loginResponse"
        type="java:logic.LoginResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>getUserNamesResponse"
        type="java:logic.GetUserNamesResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>login"
        type="java:logic.Login"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>getUserInfoResponse"
        type="java:logic.GetUserInfoResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://logic"
        qname="ns:>addUser"
        type="java:logic.AddUser"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
